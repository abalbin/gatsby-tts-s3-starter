{
  "name": "gatsby-tts-s3-default",
  "private": true,
  "description": "A starter to get up and developing quickly with Gatsby, typescript, tailwind CSS and deploying with S3",
  "version": "0.1.0",
  "author": "Arturo Balbin <a.balbin.r@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^2.32.3",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.27",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-root-import": "^6.6.0",
    "prettier": "2.2.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.5"
  },
  "keywords": [
    "gatsby",
	"typescript",
	"tailwind",
	"style-components",
	"s3"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop -p 3200 -H 0.0.0.0",
