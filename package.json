    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abalbin/gatsby-tts-s3-starter"
  },
  "bugs": {
    "url": "https://github.com/abalbin/gatsby-tts-s3-starter/issues"
